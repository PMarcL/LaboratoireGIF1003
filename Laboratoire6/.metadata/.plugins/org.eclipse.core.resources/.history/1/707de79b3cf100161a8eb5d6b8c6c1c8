#include <iostream>
#include "Temps.h"

using namespace std;
using namespace labo;

int main() {
	Temps tempsLocal;
	cout << tempsLocal.reqTempsFormate() << endl;
	cout << "Nombre d'instances : " << Temps::reqInstances() << endl;

	Temps temps(23, 59, 59);
	cout << temps.reqTempsFormate() << endl;
	cout << "Nombre d'instances : " << Temps::reqInstances() << endl;

	return 0;
}

