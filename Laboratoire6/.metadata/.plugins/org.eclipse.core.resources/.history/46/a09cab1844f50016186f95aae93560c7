#include "NombreRationnel.h"
#include <iostream>

using namespace std;
using namespace labo;

int main() {
	NombreRationnel fraction1(1, 2);
	NombreRationnel fraction2(1, 4);
	NombreRationnel fraction3 = fraction1 * fraction2;

	cout << fraction3.reqFormate() << endl;

	return 0;
}

