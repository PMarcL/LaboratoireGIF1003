#include "Sphere.h"

namespace labo8 {

Sphere::Sphere() :
		m_rayon(0) {
}

Sphere::~Sphere() {
}

double Sphere::reqRayon() const {
	return m_rayon;
}

void Sphere::asgRayon(double rayon) {
	m_rayon = rayon;
}

} /* namespace labo8 */

